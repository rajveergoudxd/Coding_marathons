   "source": [
    "# Bubble Sort\n",
    "\n",
    "lst = [45,10,34,26,5,60]\n",
    "\n",
    "def bubble_sort(arr):\n",
    "    for i in range(len(arr)):\n",
    "        for j in range(i+1):\n",
    "            if(arr[i]<arr[j]):\n",
    "                temp = arr[i]\n",
    "                arr[i] = arr[j]\n",
    "                arr[j] = temp\n",
    "\n",
    "bubble_sort(lst)\n",
    "print(lst)\n"
   ]
